<div class='container' id="showcases" data-ctrl="vpns">


  <div class="row">
    <nav>
      <ul class="pager">
        <li class="previous"> <%= link_to 'New VPN', new_vpn_path %></li>
        <li class="next"><h3 style="float: right;">Count: <span><%= @count %></span></h3></li>
      </ul>
    </nav>
  </div>

  <div class="panel panel-primary">
    <!-- Default panel contents -->
    <div class="panel-heading">Listing vpns</div>
    <div class="panel-body">

      <!-- Table -->
      <table class="table table-striped table-bordered" cellspacing="0">
        <thead>
        <tr>
          <th>Name</th>
          <th>Vpnid</th>
          <th>Show</th>
        </tr>
        </thead>

        <% @vpns.each do |vpn| %>
            <tr>
              <td><%= vpn.name %></td>
              <td><%= vpn.vpnid %></td>
              <td><%= link_to 'Show', vpn %></td>
            </tr>
        <% end %>
      </table>
    </div>
  </div>
  <div class="pagination">
    <%= will_paginate @vpns, :renderer => BootstrapPagination::Rails %>
  </div>

</div>




