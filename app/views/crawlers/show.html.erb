

<div class="row">
  <div class="container form_panel">
    <div class="col-xs-12">
      <div class="panel panel-info">
        <div class="panel-heading">
          <h4>crawler detail</h4>
        </div>
        <div class="panel-body">
          <div class="caption">
            <h3><%= @crawler.title %></h3>
          </div>
        </div>
        <div class="panel-body">
          <table class="table" style="padding: 0; margin: 0;">
            <tr>
              <td>Crawler name:</td>
              <td><%= @crawler.crawler_name %></td>
            </tr>
            <tr>
              <td>Domain:</td>
              <td><%= @crawler.domain %></td>
            </tr>
          </table>
        </div>
        <div class="panel-body">
          <%= link_to 'Back', crawlers_path, class:"btn btn-primary pull-left",type:"button" %>
          <%= link_to 'Edit', edit_crawler_path(@crawler), class:"btn btn-success pull-right",type:"button" %>
        </div>
      </div>
    </div>
  </div>
</div>




